<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    android:id="@+id/confirm_dialog_v2_constraint"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:backgroundTint="@null"
    android:background="@null"
    >
    <FrameLayout
        android:id="@+id/confirm_dialog_v2_left_frame"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/confirm_dialog_v2_right_frame"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintHorizontal_weight="1"
        android:foregroundGravity="center"
        >
        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/confirm_dialog_v2_left_bk_image"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/file_dark_green_color"
            android:layout_gravity="center"
            android:alpha="0.85"
            android:scaleType="fitXY"
            />
        <FrameLayout
            android:id="@+id/confirm_dialog_v2_left_fore_image_frame"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@null"
            android:backgroundTint="@null"
            >
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/confirm_dialog_v2_left_fore_image2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/trans"
                android:layout_gravity="center"
                android:scaleType="fitXY"
                android:tint="@color/white"
                android:layout_marginEnd="0dp"
                android:layout_marginBottom="0dp"
                />
<!--            "#5dfcf7"-->
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/confirm_dialog_v2_left_fore_image1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/trans"
                android:layout_gravity="center"
                android:scaleType="fitXY"
                />
        </FrameLayout>
<!--        app:tint="@color/red"-->
    </FrameLayout>

    <FrameLayout
        android:id="@+id/confirm_dialog_v2_right_frame"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@+id/confirm_dialog_v2_left_frame"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_weight="1"
        android:foregroundGravity="center"
        >
        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/confirm_dialog_v2_right_bk_image"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/white"
            android:layout_gravity="center"
            android:alpha="0.85"
            android:scaleType="fitXY"
            />
        <FrameLayout
            android:id="@+id/confirm_dialog_v2_right_fore_image_frame"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@null"
            android:backgroundTint="@null"
            >
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/confirm_dialog_v2_right_fore_image2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/trans"
                android:layout_gravity="center"
                android:scaleType="fitXY"
                android:tint="@color/file_dark_green_color"
                android:layout_marginStart="0dp"
                android:layout_marginBottom="0dp"
                />
<!--            "#5dfcf7"-->
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/confirm_dialog_v2_right_fore_image1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/trans"
                android:layout_gravity="center"
                android:scaleType="fitXY"
                />
        </FrameLayout>
<!--         app:tint="@color/ao"-->
    </FrameLayout>
    <FrameLayout
        android:id="@+id/confirm_dialog_v2_cancel_frame"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/confirm_dialog_v2_ok_frame"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_chainStyle="packed"
        android:layout_gravity="center"
        android:background="@color/trans"
        android:backgroundTint="@null">
        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/confirm_dialog_v2_cancel2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:minWidth="100dp"
            android:minHeight="100dp"
            app:tint="@color/file_dark_green_color"
            android:background="@color/trans"
            android:backgroundTint="@null"
            android:layout_gravity="center"
            android:scaleType="fitCenter"
            />
        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/confirm_dialog_v2_cancel1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:minWidth="100dp"
            android:minHeight="100dp"
            app:tint="@color/white"
            android:background="@color/trans"
            android:backgroundTint="@null"
            android:layout_gravity="center"
            android:scaleType="fitCenter"
            />
    </FrameLayout>
    <FrameLayout
        android:id="@+id/confirm_dialog_v2_ok_frame"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_gravity="center"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@+id/confirm_dialog_v2_cancel_frame"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:background="@color/trans"
        android:backgroundTint="@null">
        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/confirm_dialog_v2_ok2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:tint="@color/white"
            android:background="@color/trans"
            android:backgroundTint="@null"
            android:layout_gravity="center"
            android:scaleType="fitCenter"
            />
        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/confirm_dialog_v2_ok1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:tint="@color/file_dark_green_color"
            android:background="@color/trans"
            android:backgroundTint="@null"
            android:layout_gravity="center"
            android:scaleType="fitCenter"
            />
    </FrameLayout>
    <FrameLayout
        android:id="@+id/confirm_dialog_v2_click_left_frame"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/confirm_dialog_v2_click_right_frame"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintDimensionRatio="h, 1:3"
        android:clickable="true"
        android:focusable="true"
        android:foreground="?selectableItemBackground"
        android:foregroundGravity="center"
        android:layout_marginStart="40dp"
        android:layout_marginBottom="40dp"
        />
    <FrameLayout
        android:id="@+id/confirm_dialog_v2_click_right_frame"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintStart_toEndOf="@+id/confirm_dialog_v2_click_left_frame"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintDimensionRatio="h, 1:3"
        android:clickable="true"
        android:focusable="true"
        android:foreground="?selectableItemBackground"
        android:foregroundGravity="center"
        android:layout_marginEnd="40dp"
        android:layout_marginBottom="40dp"
        />
</androidx.constraintlayout.widget.ConstraintLayout>