    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/extra_popup_cmdindex_fg3_image"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:src="@mipmap/ic_cmdclick_launcher_background"
        android:scaleType="fitXY"
        android:alpha="1"
        android:layout_gravity="center"
        android:background="@null"
        android:backgroundTint="@null"/>
<!--     android:paddingLeft="10dp"
        android:paddingRight="10dp"-->
    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/extra_popup_cmdindex_fg2_image"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:src="@mipmap/ic_cmdclick_launcher_background"
        android:scaleType="fitXY"
        android:alpha="1"
        android:layout_gravity="center"
        android:background="@null"
        android:backgroundTint="@null"/>
    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/extra_popup_cmdindex_fg1_image"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:src="@mipmap/ic_cmdclick_launcher_background"
        android:scaleType="fitXY"
        android:alpha="1"
        android:layout_gravity="center"
        android:background="@null"
        android:backgroundTint="@null"/>

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/extra_popup_cmdindex_restart_ubuntu_image"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintWidth_percent="0.22"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/extra_popup_cmdindex_debugger_image"
        app:layout_constraintBottom_toTopOf="@+id/extra_popup_cmdindex_page_search_image"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintHorizontal_bias="0.4"
        android:src="@drawable/icons8_launch"
        app:tint="@color/cmdclick_text_black"
        android:clickable="true"
        android:focusable="true"
        android:foreground="?selectableItemBackground"
        android:background="@null"
        android:backgroundTint="@null"
        android:layout_gravity="center"
        android:gravity="center"
        android:layout_margin="10dp"
        android:clipChildren="false"
        android:clipToPadding="false"
        />
<!--     app:layout_constraintHorizontal_chainStyle="spread"-->
    <com.puutaro.commandclick.custom_view.OutlineTextView
        android:id="@+id/extra_popup_cmdindex_restart_ubuntu_caption"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="@+id/extra_popup_cmdindex_restart_ubuntu_image"
        app:layout_constraintStart_toStartOf="@+id/extra_popup_cmdindex_restart_ubuntu_image"
        app:layout_constraintEnd_toEndOf="@+id/extra_popup_cmdindex_restart_ubuntu_image"
        app:layout_constraintBottom_toBottomOf="@+id/extra_popup_cmdindex_restart_ubuntu_image"
        android:padding="5dp"
        android:textSize="20sp"
        android:textStyle="bold"
        android:ellipsize="marquee"
        android:textColor="@color/white"
        android:layout_marginStart="0dp"
        android:layout_marginEnd="0dp"
        android:text="rstrtUbnt"
        android:layout_gravity="center"
        android:gravity="center"
        />
    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/extra_popup_cmdindex_debugger_image"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintWidth_percent="0.22"
