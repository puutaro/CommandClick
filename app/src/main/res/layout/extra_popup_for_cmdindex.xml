        app:layout_constraintEnd_toEndOf="@+id/extra_popup_cmdindex_debugger_image"
        app:layout_constraintBottom_toBottomOf="@+id/extra_popup_cmdindex_debugger_image"
        android:padding="5dp"
        android:textSize="20sp"
        android:textStyle="bold"
        android:ellipsize="marquee"
        android:layout_marginStart="4dp"
        android:layout_marginEnd="4dp"
        android:text="debug"
        android:layout_gravity="center"
        android:gravity="center"
        />


    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/extra_popup_cmdindex_page_search_image"
        android:layout_width="0dp"
        android:layout_height="0dp"
