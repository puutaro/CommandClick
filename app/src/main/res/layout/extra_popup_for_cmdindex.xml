        app:layout_constraintBottom_toTopOf="@+id/extra_popup_cmdindex_shortcut_image"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintHorizontal_bias="0.8"
        android:clickable="true"
        android:focusable="true"
        android:orientation="vertical"
        android:foreground="?selectableItemBackground"
        android:src="@drawable/icon_debug"
        app:tint="@color/cmdclick_text_black"
        android:background="@null"
        android:backgroundTint="@null"
        android:layout_gravity="center"
        android:gravity="center"
        android:layout_margin="10dp"
        android:shadowColor="@color/file_dark_green_color"
        android:shadowDx="-2"
        android:shadowDy="-2"
        android:shadowRadius="2"
        />
    <com.puutaro.commandclick.custom_view.OutlineTextView
        android:id="@+id/extra_popup_cmdindex_debugger_caption"
        android:layout_width="0dp"
        android:layout_height="0dp"
