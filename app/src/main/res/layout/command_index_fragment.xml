<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    >
<!--    android:layout_weight="0.6"-->

    <data>

    </data>


    <androidx.appcompat.widget.LinearLayoutCompat
        android:id="@+id/command_index_fragment"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:weightSum="1"
        android:gravity="bottom"
        >
        <include
            layout="@layout/page_search"
            android:id="@+id/page_search"
            />

        <androidx.cardview.widget.CardView
            android:id="@+id/cmdindex_selection_search_button"
            android:layout_height="@dimen/toolbar_layout_height"
            android:layout_width="match_parent"
            app:cardCornerRadius="10dp"
            app:cardElevation="5dp"
            android:layout_weight="1"
            android:visibility="gone"
            android:layout_marginStart="5dp"
            android:layout_marginEnd="5dp"
            android:clickable="true"
            android:focusable="true"
            android:foreground="?selectableItemBackground"
            >
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/cmdindex_selection_search_icon1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/google"
                android:backgroundTint="@null"
                android:background="@null"
                app:tint="@null"
                android:scaleType="centerCrop"
                android:layout_gravity="center"
            />
            <com.puutaro.commandclick.custom_view.OutlineTextView
                android:id="@+id/cmdindex_selection_search_caption"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:padding="0dp"
                android:textSize="30sp"
                android:textStyle="bold"
                android:textColor="@color/fill_gray"
                android:ellipsize="end"
                android:maxLines="1"
                android:layout_gravity="center"
                android:gravity="top"
                android:text="Search"
                android:letterSpacing="0.1"
                />
            <com.puutaro.commandclick.custom_view.OutlineTextView
                android:id="@+id/cmdindex_selection_search_cur_text"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:padding="0dp"
                android:textSize="16sp"
                android:textStyle="bold"
                android:textColor="@color/white"
                android:ellipsize="end"
                android:maxLines="1"
                android:layout_gravity="center"
                android:gravity="bottom"
                android:visibility="gone"
                android:alpha="0.9"
                android:letterSpacing="0.1"
                />
        </androidx.cardview.widget.CardView>
        <androidx.appcompat.widget.LinearLayoutCompat
            android:id="@+id/cmdindex_toolbar_linear_layout"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:orientation="horizontal"
            android:layout_weight="1"
            android:weightSum="1"
            android:visibility="gone"
            >
            <FrameLayout
                android:id="@+id/cmdindex_fannel_center_button"
                android:layout_width="0dp"
                android:layout_height="@dimen/toolbar_layout_height"
                android:clickable="true"
                android:focusable="true"
                android:orientation="vertical"
                android:foreground="?selectableItemBackground"
                android:layout_weight="0.2"
                android:layout_marginStart="@dimen/toolbar_button_horizon_margin"
                android:layout_marginEnd="@dimen/toolbar_button_horizon_margin"
                >
                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/cmdindex_fannel_center_button_image"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:src="@drawable/icons8_history"
                    android:backgroundTint="@color/white"
                    app:tint="@color/file_dark_green_color"
                    android:scaleType="fitCenter"
                    android:padding="0dp"
                    android:layout_gravity="center"
                    />
                <com.puutaro.commandclick.custom_view.OutlineTextView
                    android:id="@+id/cmdindex_fannel_center_button_caption"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:padding="0dp"
                    android:textSize="@dimen/toolbar_caption_font_size"
                    android:textStyle="bold"
                    android:ellipsize="end"
                    android:maxLines="1"
                    android:text="@string/fannel_center_caption"
                    android:layout_gravity="center"
                    android:gravity="center"
                    />
            </FrameLayout>

            <FrameLayout
                android:id="@+id/cmdindex_url_history_button"
                android:layout_width="0dp"
                android:layout_height="@dimen/toolbar_layout_height"
                android:clickable="true"
                android:focusable="true"
                android:foreground="?selectableItemBackground"
                android:layout_weight="0.2"
                android:layout_marginStart="@dimen/toolbar_button_horizon_margin"
                android:layout_marginEnd="@dimen/toolbar_button_horizon_margin"
                >
                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/cmdindex_url_history_button_image"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:src="@drawable/black_history_icons"
                    android:backgroundTint="@color/white"
                    app:tint="@color/file_dark_green_color"
                    android:scaleType="fitCenter"
                    android:layout_gravity="center"
                    android:padding="0dp"
                    />
                <com.puutaro.commandclick.custom_view.OutlineTextView
                    android:id="@+id/cmdindex_url_history_button_caption"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:scaleType="fitCenter"
                    android:padding="0dp"
                    android:textSize="@dimen/toolbar_caption_font_size"
                    android:textStyle="bold"
                    android:ellipsize="end"
                    android:maxLines="1"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:text="urlHstory"
                    />
            </FrameLayout>

            <FrameLayout
                android:id="@+id/cmdindex_search_button"
                android:layout_width="0dp"
                android:layout_height="@dimen/toolbar_layout_height"
                android:clickable="true"
                android:focusable="true"
                android:foreground="?selectableItemBackground"
                android:layout_weight="0.2"
                android:layout_marginStart="@dimen/toolbar_button_horizon_margin"
                android:layout_marginEnd="@dimen/toolbar_button_horizon_margin"
                >
                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/cmdindex_search_button_image"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:src="@drawable/icons8_search"
                    android:backgroundTint="@color/white"
                    app:tint="@color/file_dark_green_color"
                    android:scaleType="fitCenter"
                    android:layout_gravity="center"
                    android:padding="0dp"
                    />
                <com.puutaro.commandclick.custom_view.OutlineTextView
                    android:id="@+id/cmdindex_search_button_caption"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:scaleType="fitCenter"
                    android:padding="0dp"
                    android:textSize="@dimen/toolbar_caption_font_size"
                    android:textStyle="bold"
                    android:ellipsize="end"
                    android:maxLines="1"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:text="sch"
                    />
            </FrameLayout>

            <FrameLayout
                android:id="@+id/cmdindex_show_clear_toolbar_button"
                android:layout_width="0dp"
                android:layout_height="@dimen/toolbar_layout_height"
                android:clickable="true"
                android:focusable="true"
                android:foreground="?selectableItemBackground"
                android:layout_weight="0.2"
                android:layout_marginStart="@dimen/toolbar_button_horizon_margin"
                android:layout_marginEnd="@dimen/toolbar_button_horizon_margin"
                >
                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/cmdindex_show_pin_button_image"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:src="@drawable/ic_top_allow"
                    android:backgroundTint="@color/white"
                    app:tint="@color/file_dark_green_color"
                    android:scaleType="fitCenter"
                    android:layout_gravity="center"
                    android:padding="0dp"
                    />
                <com.puutaro.commandclick.custom_view.OutlineTextView
                    android:id="@+id/cmdindex_show_pin_button_caption"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:scaleType="fitCenter"
                    android:padding="0dp"
                    android:textSize="@dimen/toolbar_caption_font_size"
                    android:textStyle="bold"
                    android:ellipsize="end"
                    android:maxLines="1"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:text="shwPin"
                    />
            </FrameLayout>

            <FrameLayout
                android:id="@+id/cmdindex_setting_button"
                android:layout_width="0dp"
                android:layout_height="@dimen/toolbar_layout_height"
                android:clickable="true"
                android:focusable="true"
                android:foreground="?selectableItemBackground"
                android:layout_weight="0.2"
                android:layout_marginStart="@dimen/toolbar_button_horizon_margin"
                android:layout_marginEnd="@dimen/toolbar_button_horizon_margin"
                >
                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/cmdindex_setting_button_image"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:src="@drawable/extra"
                    android:backgroundTint="@color/white"
                    app:tint="@color/file_dark_green_color"
                    android:scaleType="fitCenter"
                    android:layout_gravity="center"
                    android:padding="0dp"
                    />
                <com.puutaro.commandclick.custom_view.OutlineTextView
                    android:id="@+id/cmdindex_setting_button_caption"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:scaleType="fitCenter"
                    android:padding="0dp"
                    android:textSize="@dimen/toolbar_caption_font_size"
                    android:textStyle="bold"
                    android:ellipsize="end"
                    android:maxLines="1"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:text="extra"
                    />
            </FrameLayout>
        </androidx.appcompat.widget.LinearLayoutCompat>
    </androidx.appcompat.widget.LinearLayoutCompat>

</layout>